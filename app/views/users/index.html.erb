<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Last name</th>
      <th>First name</th>
      <th>Salutation</th>
      <th>Username</th>
      <th>Company</th>
      <th>Department</th>
      <th>Title</th>
      <th>Country</th>
      <th>City</th>
      <th>State</th>
      <th>Postalcode</th>
      <th>Address</th>
      <th>Tel</th>
      <th>Fax</th>
      <th>Mobile</th>
      <th>Url</th>
      <th>Gender</th>
      <th>Birthdate</th>
      <th>Guraduation</th>
      <th>Workexperience</th>
      <th>Language</th>
      <th>Japanese</th>
      <th>English</th>
      <th>Toeic</th>
      <th>Toefl</th>
      <th>Q01</th>
      <th>Q02</th>
      <th>Q03</th>
      <th>Q04</th>
      <th>Q05</th>
      <th>Comment</th>
      <th>Status</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.last_name %></td>
        <td><%= user.first_name %></td>
        <td><%= user.salutation %></td>
        <td><%= user.username %></td>
        <td><%= user.company %></td>
        <td><%= user.department %></td>
        <td><%= user.title %></td>
        <td><%= user.country %></td>
        <td><%= user.city %></td>
        <td><%= user.state %></td>
        <td><%= user.postalcode %></td>
        <td><%= user.address %></td>
        <td><%= user.tel %></td>
        <td><%= user.fax %></td>
        <td><%= user.mobile %></td>
        <td><%= user.url %></td>
        <td><%= user.gender %></td>
        <td><%= user.birthdate %></td>
        <td><%= user.guraduation %></td>
        <td><%= user.workexperience %></td>
        <td><%= user.language %></td>
        <td><%= user.japanese %></td>
        <td><%= user.english %></td>
        <td><%= user.toeic %></td>
        <td><%= user.toefl %></td>
        <td><%= user.Q01 %></td>
        <td><%= user.Q02 %></td>
        <td><%= user.Q03 %></td>
        <td><%= user.Q04 %></td>
        <td><%= user.Q05 %></td>
        <td><%= user.comment %></td>
        <td><%= user.status %></td>
        <td><%= user.note %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
